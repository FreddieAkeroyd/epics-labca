//Scilab functions 
ezca_funs=[...
  'ezcaGet';
  'ezcaGetString';
  'ezcaPut';
  'ezcaPutString';
  'ezcaGetNelem';
  'ezcaGetControlLimits';
  'ezcaGetGraphicLimits';
